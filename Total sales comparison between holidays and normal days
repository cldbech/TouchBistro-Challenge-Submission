    },
    {
      "cell_type": "code",
      "source": [
        "canadian_holidays = [\n",
        "    \"2023-01-01\",  # New Year's Day\n",
        "    \"2023-02-20\",  # Family Day\n",
        "    \"2023-04-07\",  # Good Friday\n",
        "    \"2023-05-22\",  # Victoria Day\n",
        "    \"2023-07-01\",  # Canada Day\n",
        "    \"2023-09-04\",  # Labour Day\n",
        "    \"2023-10-09\",  # Thanksgiving (Canada)\n",
        "    \"2023-12-25\",  # Christmas Day\n",
        "    \"2023-12-26\"   # Boxing Day\n",
        "    ]\n",
        "\n",
        "canadian_holidays = pd.to_datetime(canadian_holidays)\n",
        "bills[\"is_holiday\"] = bills[\"business_date\"].isin(canadian_holidays)"
      ],
      "metadata": {
